package com.abc.pushtrip.trade.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.abc.pushtrip.trade.mapper.TradeForumDAO;
import com.abc.pushtrip.trade.vo.TradeForumVO;

@Service
@Transactional
public class TradeForumServiceImpl implements TradeForumService {
	
	@Autowired
	public TradeForumDAO tradeForumDAO;
	
	@Override
	public int tradeForumInsert(TradeForumVO tfvo) {
		// TODO Auto-generated method stub
		return tradeForumDAO.tradeForumInsert(tfvo);
	}

	@Override
	public List<TradeForumVO> tradeForumSelectAll(TradeForumVO tfvo) {
		// TODO Auto-generated method stub
		return tradeForumDAO.tradeForumSelectAll(tfvo);
	}

	@Override
	public List<TradeForumVO> tradeForumUpdate(TradeForumVO tfvo) {
		// TODO Auto-generated method stub
		return tradeForumDAO.tradeForumUpdate(tfvo);
	}

	@Override
	public List<TradeForumVO> tradeForumDelete(TradeForumVO tfvo) {
		// TODO Auto-generated method stub
		return tradeForumDAO.tradeForumDelete(tfvo);
	}

}
